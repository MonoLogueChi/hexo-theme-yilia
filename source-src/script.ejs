<script>
<% var left = '<%=' %>
<% var right = '%' %>
<% var right2 = '>' %>
(function() {
	<% for (var asset in compilation.assets) { %>
        <% if (asset.indexOf('main') >= 0 && asset.indexOf('.js') >= 0) { %>
            <% var main = asset %>
        <% } %>
    <% } %>
    assetsLoader.load([{url:"<%= left %>config.root<%= right %><%= right2 %>assets/<%= main %>", type:'script'}]);
})();
(function() {
	<% for (var asset in compilation.assets) { %>
        <% if (asset.indexOf('slider') >= 0) { %>
            <% var slider = asset %>
        <% } %>
    <% } %>
    assetsLoader.load([{url:"<%= left %>config.root<%= right %><%= right2 %>assets/<%= slider %>", type:'script'}]);
})();
</script>